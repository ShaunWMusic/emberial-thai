  <h2 class="title title-margin">Categories</h2>
  <div class="card">
  <div class="btn-right">{{#link-to "admin.category.new" class="btn"}}Add New{{/link-to}}</div>
  <h4 class="category">Name</h4>
  <ul class="category-items">
  {{#each model as |category|}}
      <div class="more-info">
        <li>{{ category.name }}</li>
        {{#link-to "admin.category.detail" category.id}}detail{{/link-to}}
      </div>
  {{/each}}
  </ul>
  </div>
